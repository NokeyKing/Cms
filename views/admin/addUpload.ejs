<!DOCTYPE html>
<html>
<head>
    <!-- 顶部公共css和js -->
    <% include top.ejs%>
    <!-- 顶部公共css和js -->
</head>
<body>
<!-- 公共头部 -->
<% include header.ejs%>
<!-- 公共头部 -->

<div class="ch-container">
    <div class="row">

        <!-- 左侧 -->
        <% include left.ejs%>
        <!-- 左侧 -->

        <div id="content" class="col-lg-10 col-sm-10">
            <!-- 面包 -->
            <div>
                <ul class="breadcrumb">
                    <li>
                        <a href="/admin/">Home</a>
                    </li>
                    <li>
                        <a href="/admin/articleList/">ArticleList</a>
                    </li>
                </ul>
            </div>
            <!-- 面包 -->
            <!-- 类别列表 -->

            <%- message %>
            <div class="row">
                <div class="box col-md-12">
                    <div class="box-inner">
                        <div class="box-header well" data-original-title="">
                            <h2><i class="glyphicon glyphicon-edit"></i> <%=title%></h2>

                            <div class="box-icon">
                                <!--<a href="#" class="btn btn-setting btn-round btn-default"><i class="glyphicon glyphicon-cog"></i></a>-->
                                <a href="#" class="btn btn-minimize btn-round btn-default"><i class="glyphicon glyphicon-chevron-up"></i></a>
                                <a href="#" class="btn btn-close btn-round btn-default"><i class="glyphicon glyphicon-remove"></i></a>
                            </div>
                        </div>
                        <div class="box-content">
                            <form method="post" class="form-horizontal" enctype="multipart/form-data">
                                <div class="form-group">
                                    <div class="col-lg-12">
                                        <input type="text" class="form-control" name="uploadTitle" placeholder="上传文件名称">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-12">
                                        <input type="file" class="form-control" name="upload" multiple="multiple">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-12">
                                        <input name="" type="submit" class="btn btn-primary mr15" value="上传"/>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <!--/span-->

            </div>
            <!-- 类别列表 -->
        </div>
    </div>
    <% include footer.ejs%>
</div>

<!-- 底部公共css和js -->
<% include bottom.ejs%>
<!-- 底部公共css和js -->
</body>
</html>